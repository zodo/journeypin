{
	"printWidth": 100,
	"singleQuote": true,
	"semi": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"quoteProps": "as-needed",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"tailwindConfig": "./frontend/tailwind.config.js",
	"tailwindFunctions": ["clsx", "cn"]
}
